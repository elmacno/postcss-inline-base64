{
  "name": "postcss-inline-base64",
  "version": "4.0.0",
  "description": "PostCSS plugin for encode the file to base64",
  "keywords": [
    "postcss",
    "css",
    "postcss-plugin",
    "base64",
    "file",
    "inline",
    "postcss-inline-base64"
  ],
  "author": "Thiago Lagden <lagden@gmail.com>",
  "license": "MIT",
  "main": "index.js",
  "repository": "lagden/postcss-inline-base64",
  "bugs": {
    "url": "https://github.com/lagden/postcss-inline-base64/issues"
  },
  "homepage": "https://github.com/lagden/postcss-inline-base64",
  "engines": {
    "node": ">=8.4.0"
  },
  "scripts": {
    "report": "nyc report --reporter=lcov | codecov",
    "pretest": "xo",
    "test": "ava"
  },
  "xo": {
    "envs": [
      "node",
      "es6"
    ],
    "semicolon": false,
    "space": false,
    "rules": {
      "capitalized-comments": 0
    }
  },
  "dependencies": {
    "debug": "3.1.0",
    "file-type": "8.1.0",
    "got": "8.3.2",
    "is-svg": "3.0.0",
    "p-map": "1.2.0",
    "postcss": "7.0.1"
  },
  "devDependencies": {
    "ava": "0.25.0",
    "nyc": "12.0.2",
    "xo": "0.21.1"
  },
  "files": [
    "index.js",
    "README.md",
    "CHANGELOG.md",
    "LICENSE"
  ]
}
