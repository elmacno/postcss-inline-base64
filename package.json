{
  "name": "postcss-inline-base64",
  "version": "4.1.0",
  "description": "PostCSS plugin for encode the file to base64",
  "keywords": [
    "postcss",
    "css",
    "postcss-plugin",
    "base64",
    "file",
    "inline",
    "postcss-inline-base64"
  ],
  "author": "Thiago Lagden <lagden@gmail.com>",
  "license": "MIT",
  "main": "index.js",
  "repository": "lagden/postcss-inline-base64",
  "bugs": {
    "url": "https://github.com/lagden/postcss-inline-base64/issues"
  },
  "homepage": "https://github.com/lagden/postcss-inline-base64",
  "engines": {
    "node": ">=8.4.0"
  },
  "scripts": {
    "report": "nyc report --reporter=lcov | codecov",
    "pretest": "xo",
    "test": "ava"
  },
  "xo": {
    "envs": [
      "node",
      "es6"
    ],
    "semicolon": false,
    "space": false,
    "rules": {
      "capitalized-comments": 0
    }
  },
  "dependencies": {
    "debug": "4.1.1",
    "file-type": "12.4.0",
    "got": "9.6.0",
    "is-svg": "4.2.0",
    "p-map": "3.0.0",
    "postcss": "7.0.21"
  },
  "devDependencies": {
    "ava": "2.4.0",
    "nyc": "14.1.1",
    "xo": "0.25.3"
  },
  "files": [
    "index.js",
    "README.md",
    "CHANGELOG.md",
    "LICENSE"
  ]
}
