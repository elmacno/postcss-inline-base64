{
  "name": "postcss-inline-base64",
  "version": "3.0.0",
  "description": "PostCSS plugin for encode the file to base64",
  "keywords": [
    "postcss",
    "css",
    "postcss-plugin",
    "base64",
    "file",
    "inline",
    "postcss-inline-base64"
  ],
  "author": "Thiago Lagden <lagden@gmail.com>",
  "license": "MIT",
  "main": "index.js",
  "repository": "lagden/postcss-inline-base64",
  "bugs": {
    "url": "https://github.com/lagden/postcss-inline-base64/issues"
  },
  "homepage": "https://github.com/lagden/postcss-inline-base64",
  "engines": {
    "node": ">=8.4.0"
  },
  "scripts": {
    "report": "nyc report --reporter=html",
    "coverage": "nyc report --reporter=lcov | codecov",
    "pretest": "xo",
    "test": "nyc ava --serial",
    "debug": "DEBUG_COLORS=1 DEBUG=b64:* npm test"
  },
  "xo": {
    "esnext": true,
    "extends": "eslint-config-postcss",
    "semicolon": false,
    "space": false,
    "rules": {
      "capitalized-comments": 0
    }
  },
  "dependencies": {
    "debug": "3.1.0",
    "file-type": "7.5.0",
    "got": "8.1.0",
    "is-svg": "3.0.0",
    "postcss": "6.0.18"
  },
  "devDependencies": {
    "ava": "0.25.0",
    "codecov": "3.0.0",
    "eslint-config-postcss": "2.0.2",
    "nyc": "11.4.1",
    "rmdir": "1.2.0",
    "xo": "0.20.1"
  },
  "files": [
    "index.js",
    "README.md",
    "CHANGELOG.md",
    "LICENSE"
  ]
}
